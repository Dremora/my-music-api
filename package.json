    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "micro": "^9.3.4",
    "nexus": "^1.3.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
