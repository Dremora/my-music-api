{
  "name": "my-music-api",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/Dremora/my-music-api",
  "author": "Kirill Korolyov <kirill.korolyov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only -- ./src/server.ts",
    "dev:typecheck": "tsc --noEmit --watch",
    "prisma:generate": "prisma generate",
    "prisma:introspect": "prisma introspect",
    "types:generate": "ts-node --transpile-only src/schema",
    "db:migrate": "prisma migrate deploy",
    "build": "tsc",
    "start": "NODE_ENV=production node .",
    "lint": "eslint . --ext ts"
  },
  "dependencies": {
    "@prisma/client": "^3.2.1",
    "apollo-server": "^3.3.0",
    "apollo-server-micro": "^3.4.0",
    "date-fns": "^2.25.0",
    "graphql": "^15.6.1",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-no-type-assertion": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "prettier": "^2.4.1",
    "prisma": "^3.2.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "14.x"
  }
}
